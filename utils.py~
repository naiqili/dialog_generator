import random

def random_select(lst):
    s = sum(v for (k, v) in lst)
    uniform_lst = [(k, v/s) for (k, v) in lst]
    print uniform_lst
    p = 0
    rnd = random.random()
    for (k, v) in unform_lst:
        p = p + v
        if p >= rnd:
            return k
    return uniform_lst[-1][0]


if __name__=='__main__':
    for k in range(20):
        print random_select([("high", 0.6), ("median", 0.3), ("low", 0.1)])
